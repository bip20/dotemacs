${1:context}.strokeStyle = "${2:borderColor}";
$1.fillStyle = "${3:backgroundColor}";
$1.beginPath();
$1.arc(${4:x}, ${5:y}, ${6:width}, $6, Math.PI*2, true);
$1.closePath();
$1.stroke();
$1.fill();
